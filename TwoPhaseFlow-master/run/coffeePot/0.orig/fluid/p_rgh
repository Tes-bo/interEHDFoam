FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    Sym
    {
        type fixedFluxPressure;
    }

    fluid_to_solid
    {
        type fixedFluxPressure;
    }

    top
    {
        type totalPressure;
        p0 uniform 1e5;
        value uniform 1e5;
    }

    bottomEmptyFaces
    {
        type empty;
    }

    topEmptyFaces
    {
        type empty;
    }
}

