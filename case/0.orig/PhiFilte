FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      PhiFilte;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }

    outlet
    {
        type            calculated;
        value           uniform 0;
    }

    FACE 
    {
        type            wedge;
    }

    FACE_2
    {
        type            wedge;
    }

    wall
    {
        type            calculated;
        value           uniform 0;
    }

    free
    {
        type            calculated;
        value           uniform 0;
    }

    defaultFaces
    {
        type            empty;
    }
}


